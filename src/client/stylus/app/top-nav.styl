//navbar
#top-nav
  position absolute
  background #080808 
  height 34px
  width 100%
  no-around()
//items on navbar
#top-nav div
  float right
  position relative
#top-nav a
  display block
  padding 7px 
  margin-right 7px 
  background none
  color white
  font-size 15px
  text-decoration none
  &:hover
    background green 
#topnav-title
  font-size 23px !important
  padding-top 5px !important
  margin-left 7px
  


